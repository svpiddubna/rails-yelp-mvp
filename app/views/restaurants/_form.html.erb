<%= simple_form_for(@restaurant) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category, priority: [ "italian" ], collection: ["chinese", "italian", "japanese", "french", "belgian"] %>
    <%= f.submit :submit, class: "btn btn-success" %>
  <% end %>
